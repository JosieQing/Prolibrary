package com.neusoft.bean;

import java.sql.Date;

/**
 * 试用期管理
 * @author ttc
 *
 */
public class ProbationMan {
	private Integer pmid;//试用员工ID
	
	private Integer eid;//员工编号
	
	private String remarks;//考核评语
	
	private String result;//考核结果：转正、延期、不予录用
	
	private Date probationDay;//开始试用日期
	
	private Integer probationTime;//试用时间
	
	private String state;//试用状态
	
	private Date formalDay;//转正日期
	
	private Date leaveDay;//离职日期
	
	private Date delayDay;//延期日期
		
	private Date endDay;//结束日期
	
	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public ProbationMan(){
		
	}
	
	public ProbationMan(Date probationDay){
		this.probationDay = probationDay;
	}

	public Integer getPmid() {
		return pmid;
	}
	
	public void setPmid(Integer pmid) {
		this.pmid = pmid;
	}
	
	public Integer getEid() {
		return eid;
	}
	
	public void setEid(Integer eid) {
		this.eid = eid;
	}
	
	
	public String getResult() {
		return result;
	}
	
	public void setResult(String result) {
		this.result = result;
	}
	
	public Date getProbationDay() {
		return probationDay;
	}
	
	public void setProbationDay(java.util.Date date) {
		this.probationDay = (Date) date;
	}

	public Integer getProbationTime() {
		return probationTime;
	}

	public void setProbationTime(Integer probationTime) {
		this.probationTime = probationTime;
	}

	public Date getFormalDay() {
		return formalDay;
	}

	public void setFormalDay(Date formalDay) {
		this.formalDay = formalDay;
	}

	public Date getLeaveDay() {
		return leaveDay;
	}

	public void setLeaveDay(Date leaveDay) {
		this.leaveDay = leaveDay;
	}

	public Date getDelayDay() {
		return delayDay;
	}

	public void setDelayDay(Date delayDay) {
		this.delayDay = delayDay;
	}

	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public Date getEndDay() {
		return endDay;
	}

	public void setEndDay(Date endDay) {
		this.endDay = endDay;
	}
	
	
	
	
	
}
